# AWS認証情報
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
SECRET_KEY=your_secret_key

# API Gateway設定
API_GATEWAY_ID=your_api_gateway_id
ROOT_RESOURCE_ID=your_root_resource_id
DEVICES_RESOURCE_ID=your_devices_resource_id

# AWS設定
AWS_REGION=ap-northeast-1
DYNAMODB_TABLE_NAME=devices

# データベース設定
DB_TYPE=rds

# MySQL設定（ローカル開発用）
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_local_password
DB_NAME=lambdadb

# MySQL設定（本番環境用）
RDS_HOST=your-rds-endpoint.region.rds.amazonaws.com
RDS_PORT=3306
RDS_USER=your_rds_user
RDS_PASSWORD=your_rds_password
RDS_DATABASE=lambdadb

# 接続プール設定
DB_POOL_SIZE=5

# ログ設定
LOG_LEVEL=INFO 